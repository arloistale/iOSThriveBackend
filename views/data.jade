body
  h1= collection
  form(action="/cards", method="post", enctype="multipart/form-data")
    input(type="text", name="message", placeholder="Message Text")
    input(type="text", name="photoId", placeholder="Photo ID Number")
    input(type="submit")
  #objects
    table(border=1)
      if objects.length > 0
        - each val, key in objects[0]
          th= key
      - each obj in objects
        tr.obj
          - each val, key in obj
            td.key= val
            
